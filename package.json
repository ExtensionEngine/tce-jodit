{
  "name": "@extensionengine/tce-jodit",
  "version": "0.2.0",
  "description": "Tailor content element for Jodit editor",
  "homepage": "https://github.com/ExtensionEngine/tce-jodit#readme",
  "bugs": {
    "url": "https://github.com/ExtensionEngine/tce-jodit/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExtensionEngine/tce-jodit.git"
  },
  "license": "MIT",
  "author": "ExtensionEngine <info@extensionengine.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/tce-jodit.js",
  "umd:main": "./dist/tce-jodit.umd.js",
  "module": "./dist/tce-jodit.mjs",
  "exports": {
    ".": {
      "import": "./dist/tce-jodit.mjs",
      "require": "./dist/tce-jodit.js"
    },
    "./tce-jodit.css": "./dist/tce-jodit.css"
  },
  "scripts": {
    "prebuild": "del-cli dist",
    "build": "vite build",
    "serve": "vite serve ./example/main.js",
    "lint": "npm run lint:js && npm run lint:scss",
    "lint:js": "eslint --ext .js,.vue .",
    "lint:scss": "stylelint \"**/*.vue\" \"**/*.scss\"",
    "postshrinkwrap": "rewrite-lockfile package-lock.json"
  },
  "dependencies": {
    "auto-bind": "^5.0.1",
    "brace": "^0.11.1",
    "jodit-vue": "^3.0.0",
    "js-beautify": "^1.14.8",
    "scrollparent": "^2.1.0"
  },
  "devDependencies": {
    "@babel/eslint-plugin": "^7.22.5",
    "@extensionengine/rollup-plugin-tailor-ce": "^0.4.0",
    "@mdi/font": "^7.2.96",
    "@rollup/plugin-alias": "^5.0.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/cli-service": "^5.0.8",
    "core-js": "^3.31.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-semistandard": "^17.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-require-sort": "^1.3.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^9.15.0",
    "husky": "^8.0.3",
    "rewrite-lockfile": "^1.0.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-visualizer": "^5.9.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.63.4",
    "sass-loader": "^13.3.2",
    "stylelint": "^15.8.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "vue": "^3.3.4",
    "vue-template-compiler": "^2.7.14"
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "tailor": {
    "label": "Html",
    "type": "JODIT_HTML",
    "ui": {
      "icon": "mdi-text",
      "forceFullWidth": false
    }
  }
}
